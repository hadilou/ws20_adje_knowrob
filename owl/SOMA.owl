        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Object"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Design"/>
        <rdfs:comment>Relates an object to the design according to which it was constructed.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#hasDisposition -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#hasDisposition">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasQuality"/>
        <owl:inverseOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#isDispositionOf"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Object"/>
        <rdfs:range rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Disposition"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#hasEndLink -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#hasEndLink">
