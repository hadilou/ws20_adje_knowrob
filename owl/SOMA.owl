        <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Color"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isConceptAffordedBy -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isConceptAffordedBy">
