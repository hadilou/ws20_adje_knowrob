        <rdfs:range rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#JointState"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relates a joint to its state.</rdfs:comment>
