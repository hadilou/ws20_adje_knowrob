        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
        <rdfs:range rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Color"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#hasDesign -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#hasDesign">
