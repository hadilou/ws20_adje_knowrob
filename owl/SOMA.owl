        <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Disposition"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Object"/>
    </owl:ObjectProperty>
    


