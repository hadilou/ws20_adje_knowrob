        <rdfs:range rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Affordance"/>
        <rdfs:comment>Relates an affordance which is a relation between a bearer and a trigger, to the role of the bearer when the affordance is manifested.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isCausedByEvent -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isCausedByEvent">
