        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SpaceRegion"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has space parameter</rdfs:label>
    </owl:DatatypeProperty>
    


