        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Concept"/>
        <rdfs:range rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Affordance"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isDepositOf -->

