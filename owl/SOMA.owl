        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Concept"/>
        <rdfs:range rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Disposition"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isDefinedInAffordance"/>
            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#describes"/>
        </owl:propertyChainAxiom>
        <rdfs:comment>A relation between a disposition and a concept defined in the affordance that describes the disposition.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isConceptIncludedIn -->

