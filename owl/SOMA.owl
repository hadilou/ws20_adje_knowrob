        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isBearerDefinedIn"/>
            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#describes"/>
        </owl:propertyChainAxiom>
        <rdfs:comment>Relates a disposition to the bearer role defined by the affordance describing the disposition.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isBearerDefinedIn -->

