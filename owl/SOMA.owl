        <rdfs:range rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Joint"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relates a joint to the link it connects which is closer to the end of the kinematic chain.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is child link of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isColorOf -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isColorOf">
