        <owl:inverseOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#isEndLinkOf"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relates an object to kinematic components at the end of the kinematic chain.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has end link</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#hasExecutionState -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#hasExecutionState">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasRegion"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Action"/>
        <rdfs:range rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#ExecutionStateRegion"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#hasFeature -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#hasFeature">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasConstituent"/>
        <owl:inverseOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#isFeatureOf"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
        <rdfs:range rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Feature"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#hasFirstStep -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#hasFirstStep">
