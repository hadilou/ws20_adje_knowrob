        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
        <rdfs:comment>A spatial relation holding between an object (the deposit), and objects that are located ontop of it.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isOutputParameterFor -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isOutputParameterFor">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isParameterFor"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Parameter"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Concept"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isParameterDefinedIn -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isParameterDefinedIn">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isDefinedIn"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Parameter"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Description"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isParentLinkOf -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isParentLinkOf">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isConstituentOf"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
        <rdfs:range rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Joint"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relates a joint to the link it connects which is closer to the root of the kinematic chain.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is parent link of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isPerformedBy -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isPerformedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#involvesAgent"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Action"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isPhysicallyContainedIn -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isPhysicallyContainedIn">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isLocationOf"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A spatial relation holding between an object (the container), and objects it contains.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is physically contained in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isPlaceInvolvedIn -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isPlaceInvolvedIn">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isParticipantIn"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalPlace"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isPlanFor -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isPlanFor">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#describes"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Plan"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task"/>
        <rdfs:comment>A special relation between a Plan and a Task, to indicate that the Plan describes a way to achieve the Task.</rdfs:comment>
        <rdfs:seeAlso>Note that DUL already says Plans can define Tasks, but that relation is used differently: a Plan defines a Task because it contains it as a step.</rdfs:seeAlso>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isProcessDefinedIn -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isProcessDefinedIn">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isDefinedIn"/>
        <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#ProcessType"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Description"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isProcessTypeOf -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isProcessTypeOf">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isRelatedToConcept"/>
        <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#ProcessType"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role"/>
        <rdfs:comment>A relation between roles and process types, e.g. a catalysator is needed to trigger some chemical reaction.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isQualeOf -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isQualeOf">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isRegionFor"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Region"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Quality"/>
        <rdfs:comment>Relates a quality to its &quot;value&quot;, called quale, which is an atomic quality region.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isQualityDescribedBy -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isQualityDescribedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isDescribedBy"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Quality"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Description"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isRootLinkOf -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isRootLinkOf">
        <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#isLinkOf"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relates an object to kinematic components at the root of the kinematic chain.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is root link of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isSetpointAffordedBy -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isSetpointAffordedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#isConceptAffordedBy"/>
        <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Setpoint"/>
        <rdfs:range rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Disposition"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isSetpointDefinedIn"/>
            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#describes"/>
        </owl:propertyChainAxiom>
        <rdfs:comment>Relates a disposition to the setpoint parameter defined by the affordance describing the disposition.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isSetpointDefinedIn -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isSetpointDefinedIn">
        <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#isParameterDefinedIn"/>
        <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Setpoint"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Description"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isShapeOf -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isShapeOf">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isQualityOf"/>
        <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Shape"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isShapeRegionOf -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isShapeRegionOf">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isRegionFor"/>
        <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#ShapeRegion"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Shape"/>
                    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isSituationIncludedIn -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isSituationIncludedIn">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasSetting"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isSpaceRegionFor -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isSpaceRegionFor">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isRegionFor"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SpaceRegion"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ease-crc.org/ont/SOMA.owl#Localization"/>
                    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isStateTypeOf -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isStateTypeOf">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isRelatedToConcept"/>
        <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#StateType"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role"/>
        <rdfs:comment>A relation between roles and state types, e.g. &apos;the chair is the supporter of the person sitting on it&apos;.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isStepOf -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isStepOf">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isTaskDefinedIn"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Workflow"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is step of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isSupportedBy -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isSupportedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#interactsWith"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasCommonBoundary"/>
        <owl:inverseOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#supports"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
        <rdfs:comment>A relation between an object (the supporter) and another object (the supportee) where the supporter cancels the effect of gravity on the supportee.</rdfs:comment>
        <rdfs:comment>Relates a supportee to one of its supporters.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isTaskAffordedBy -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isTaskAffordedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#isConceptAffordedBy"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task"/>
        <rdfs:range rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Disposition"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isTaskDefinedInAffordance"/>
            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#describes"/>
        </owl:propertyChainAxiom>
        <rdfs:comment>Relates a disposition to the task defined by the affordance describing the disposition.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isTaskDefinedInAffordance -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isTaskDefinedInAffordance">
        <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#isDefinedInAffordance"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isTaskDefinedIn"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task"/>
        <rdfs:range rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Affordance"/>
        <rdfs:comment>A relation between a Task and an Affordance, such that the task is defined in terms of using the affordance.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isTerminalSceneOf -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isTerminalSceneOf">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isObjectIncludedIn"/>
        <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Scene"/>
        <rdfs:range rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#StateTransition"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between StateTransitions and Scenes, which identifies the scene the transition is expected to end at.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isTerminalStateOf -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isTerminalStateOf">
        <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#isSituationIncludedIn"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Transition"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isTriggerAffordedBy -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isTriggerAffordedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#isConceptAffordedBy"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role"/>
        <rdfs:range rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Disposition"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isTriggerDefinedIn"/>
            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#describes"/>
        </owl:propertyChainAxiom>
        <rdfs:comment>Relates a disposition to the trigger role defined by the affordance describing the disposition.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isTriggerDefinedIn -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isTriggerDefinedIn">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isRoleDefinedIn"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role"/>
        <rdfs:range rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#Affordance"/>
        <rdfs:comment>Relates an affordance which is a relation between a bearer and a trigger, to the role of the trigger when the affordance is manifested.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#meets -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#meets">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#directlyPrecedes"/>
        <owl:inverseOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#metBy"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
        <symbol rdf:datatype="http://www.w3.org/2001/XMLSchema#string">m</symbol>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between entities, expressing a &apos;sequence&apos; schema where one of the entities exactly ends where the other entity starts.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#metBy -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#metBy">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#directlyFollows"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
        <symbol rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mi</symbol>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between entities, expressing a &apos;sequence&apos; schema where one of the entities exactly ends where the other entity starts.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#movesObject -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#movesObject">
        <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#interactsWith"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
        <rdfs:comment>A relation from an agent to an object that the agent causes to move.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#overlappedBy -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#overlappedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#overlaps"/>
        <owl:inverseOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#overlappedOn"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
        <symbol rdf:datatype="http://www.w3.org/2001/XMLSchema#string">oi</symbol>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A schematic relation between any entities that also implies ordering, e.g. &quot;she has worked into the night&quot;.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#overlappedOn -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#overlappedOn">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#overlaps"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
