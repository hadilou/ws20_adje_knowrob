        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
        <rdfs:comment>Relates a localization quality to the object the localization belongs to.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#isMassAttributeOf -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#isMassAttributeOf">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isRegionFor"/>
        <rdfs:domain rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#MassAttribute"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between physical objects and their mass.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is mass attribute of</rdfs:label>
    </owl:ObjectProperty>
    


