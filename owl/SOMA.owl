        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
        <rdfs:range rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#FrictionAttribute"/>
        <rdfs:comment>A relation between physical objects and their friction attribute.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#hasGoal -->

