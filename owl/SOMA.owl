        <owl:inverseOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#isProcessDefinedIn"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Description"/>
        <rdfs:range rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#ProcessType"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ease-crc.org/ont/SOMA.owl#definesSetpoint -->

    <owl:ObjectProperty rdf:about="http://www.ease-crc.org/ont/SOMA.owl#definesSetpoint">
        <rdfs:subPropertyOf rdf:resource="http://www.ease-crc.org/ont/SOMA.owl#definesParameter"/>
